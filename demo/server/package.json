{
  "name": "@47ng/horcrux-demo-server",
  "version": "0.0.1",
  "description": "Demo server for Horcrux, using micro",
  "author": {
    "name": "Francois Best",
    "email": "contact@francoisbest.com",
    "url": "https://francoisbest.com"
  },
  "homepage": "https://github.com/47ng/horcrux",
  "license": "MIT",
  "main": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/47ng/horcrux.git"
  },
  "scripts": {
    "start": "run-p _start:foo _start:bar _start:egg",
    "start:fail": "run-p _start:foo _start:bar _start:spam",
    "_start:foo": "PORT=3001 HORCRUX_SERVER_CONFIG=v1.horcrux-server.aDmm1etftpU=. ts-node ./src/index.ts",
    "_start:bar": "PORT=3002 HORCRUX_SERVER_CONFIG=v1.horcrux-server.FstDqdcB36o=. ts-node ./src/index.ts",
    "_start:egg": "PORT=3003 HORCRUX_SERVER_CONFIG=v1.horcrux-server.0Bbv-Ys-PuU=. ts-node ./src/index.ts",
    "_start:spam": "PORT=3003 HORCRUX_SERVER_CONFIG=v1.horcrux-server.q1HBdjuodcs=. ts-node ./src/index.ts"
  },
  "bugs": {
    "url": "https://github.com/47ng/horcrux/issues"
  },
  "dependencies": {
    "@47ng/horcrux-adapter-micro": "^0.0.1",
    "micro": "^9.3.4",
    "npm-run-all": "^4.1.5",
    "then-sleep": "^1.0.1"
  }
}
